<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<!-- 每个class对应一个持久化对象 -->
	<class name="com.android.server.po.user" table="user" >
		<!-- id元素用来定义主键标识,并指定主键自动递增 -->
		<id name="id">
			<generator class="identity"></generator>
		</id>
		<!-- 定义其他属性 -->
		<property name="name" column="name"  type="string" ></property>
		<property name="password" column="password"  type="string" ></property>
		<property name="sex" column="sex"  type="string" ></property>
		<property name="age" column="age"  type="int" ></property>
		<property name="address" column="address"  type="string" ></property>
		<property name="registertime" column="registertime"  type="string" ></property>
		<property name="question" column="question"  type="string" ></property>
		<property name="marry" column="marry"  type="string" ></property>
		<property name="hobby" column="hobby"  type="string" ></property>
		<property name="email" column="email"  type="string" ></property>
		<property name="answer" column="answer"  type="string" ></property>
	</class>
</hibernate-mapping>